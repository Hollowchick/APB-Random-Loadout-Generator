                Select Case Secondary
                    Case 13
                        picSecondary.Image = My.Resources.legendary_4
                    Case 14
                        picSecondary.Image = My.Resources.legendary_8
                    Case 15
                        picSecondary.Image = My.Resources.legendary_11
                    Case Else
                        picSecondary.Image = My.Resources.ResourceManager.GetObject("secondary_" & Secondary.ToString())
