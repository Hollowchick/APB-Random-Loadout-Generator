                GunList.Clear()
            End If
        Else : picSecondary.Image = My.Resources.Empty
        End If

        'Nade Randomization
        If StatNade = True Then
            NadeList.Clear()
            If My.Settings.Nade1 = 1 Then
                NadeList.Add(1)
