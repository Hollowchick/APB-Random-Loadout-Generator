                        ChallengeLabel2.Text = "Teabag enemies visibly before shooting"
                    Case 10
                        ChallengeLabel1.Text = "This isn't CSS bunnyhop!"
                        ChallengeLabel2.Text = "Don't use your jump-key"
                    Case 11
                        ChallengeLabel1.Text = "My knees don't work like they used to"
                        ChallengeLabel2.Text = "Don't use your crouch button"
                    Case 12
                        ChallengeLabel1.Text = "Didn't mean to go there"
                        ChallengeLabel2.Text = "Swap your keys for walking left and right"
                    Case 13
                        ChallengeLabel1.Text = "No going back!"
                        ChallengeLabel2.Text = "Unbind your walk backwards key!"
                    Case 14
                        ChallengeLabel1.Text = "A real classic"
                        ChallengeLabel2.Text = "Secondary only time!"
                    Case 15
                        ChallengeLabel1.Text = "How do gaem wurk"
                        ChallengeLabel2.Text = "Don't use consumables or orange char mods"
                    Case 16
                        ChallengeLabel1.Text = ""
                        ChallengeLabel2.Text = ""
                    Case 17
                        ChallengeLabel1.Text = ""
                        ChallengeLabel2.Text = ""
                    Case Else
                        ChallengeLabel1.Text = ""
                        ChallengeLabel2.Text = ""
                End Select
            Else
                ChallengeLabel1.Text = ""
                ChallengeLabel2.Text = ""
            End If
        Else
            ChallengeLabel1.Text = ""
            ChallengeLabel2.Text = ""
        End If

        'Hard Mode
        If My.Settings.HardMode = 1 Then
            If WeaponCategory = "1" Or WeaponCategory = "4" Or WeaponCategory = "5" Then
                If Color1 = "2" Or Color2 = "2" Or Color3 = "2" Then
                    HardModeInt = generator.Next(1, 5)
                    If HardModeInt = "1" Then
                        If Color1 = "2" Then
                            GunMod1 = generator.Next(2, 5)
                            picMod1.Image = My.Resources.ResourceManager.GetObject("yellow_" & GunMod1.ToString())
                        ElseIf Color2 = "2" Then
                            GunMod2 = generator.Next(2, 5)
                            picMod2.Image = My.Resources.ResourceManager.GetObject("yellow_" & GunMod2.ToString())
                        ElseIf Color3 = "2" Then
                            GunMod3 = generator.Next(2, 5)
                            picMod3.Image = My.Resources.ResourceManager.GetObject("yellow_" & GunMod3.ToString())
                        End If
                    ElseIf HardModeInt = "2" Or HardModeInt = "3" Then
                        If Color1 = "2" Then
                            GunMod1 = generator.Next(5, 8)
                            picMod1.Image = My.Resources.ResourceManager.GetObject("yellow_" & GunMod1.ToString())
                        ElseIf Color2 = "2" Then
                            GunMod2 = generator.Next(5, 8)
                            picMod2.Image = My.Resources.ResourceManager.GetObject("yellow_" & GunMod2.ToString())
                        ElseIf Color3 = "2" Then
                            GunMod3 = generator.Next(5, 8)
                            picMod3.Image = My.Resources.ResourceManager.GetObject("yellow_" & GunMod3.ToString())
                        End If
                    Else
                        If Color1 = "2" Then
                            picMod1.Image = My.Resources.yellow_1
                        ElseIf Color2 = "2" Then
                            picMod2.Image = My.Resources.yellow_1
                        ElseIf Color3 = "2" Then
                            picMod3.Image = My.Resources.yellow_1
                        End If
                    End If
                End If
            ElseIf WeaponCategory = "6" And (Color1 = "2" Or Color2 = "2" Or Color3 = "2") Then
                If (GunMod1 = "1" Or GunMod1 = "2" Or GunMod1 = "3" Or GunMod1 = "4") And Color1 = "2" Then
                    GunMod1 = generator.Next(5, 8)
                    picMod1.Image = My.Resources.ResourceManager.GetObject("yellow_" & GunMod1.ToString())
                ElseIf (GunMod2 = "1" Or GunMod2 = "2" Or GunMod2 = "3" Or GunMod2 = "4") And Color2 = "2" Then
                    GunMod2 = generator.Next(5, 8)
                    picMod2.Image = My.Resources.ResourceManager.GetObject("yellow_" & GunMod2.ToString())
                ElseIf (GunMod3 = "1" Or GunMod3 = "2" Or GunMod3 = "3" Or GunMod3 = "4") And Color3 = "2" Then
                    GunMod3 = generator.Next(5, 8)
                    picMod3.Image = My.Resources.ResourceManager.GetObject("yellow_" & GunMod3.ToString())
                End If
            End If
            If Color1 = "2" Or Color2 = "2" Or Color3 = "2" Then
                HardModeInt = generator.Next(1, 4)
                If HardModeInt = "1" Then
                    If Color1 = "2" Then
                        picMod1.Image = My.Resources.yellow_1
                    ElseIf Color2 = "2" Then
                        picMod2.Image = My.Resources.yellow_1
                    ElseIf Color3 = "2" Then
                        picMod3.Image = My.Resources.yellow_1
                    End If
                End If
            End If
        End If
    End Sub

    'Click Events
    Private Sub picWeapon_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles picWeapon.Click
        If StatWeapon = False Then
            StatWeapon = True
            picWeapon.BackgroundImage = My.Resources.Background_ModBorderSelected
        ElseIf StatWeapon = True Then
            StatWeapon = False
            picWeapon.BackgroundImage = My.Resources.Background_ModBorder
        End If
    End Sub

    Private Sub picMod1_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles picMod1.Click
        If StatMod1 = False Then
            StatMod1 = True
            picMod1.BackgroundImage = My.Resources.Background_ModBorderSelectedThick
        ElseIf StatMod1 = True Then
            StatMod1 = False
            picMod1.BackgroundImage = My.Resources.Background_ModBorderThick
        End If
    End Sub

